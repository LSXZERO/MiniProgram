/* pages/guide/guide.wxss */
.bb, .bb::before, .bb::after {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.bb {
  width: 150px;
  height: 150px;
  margin: auto;
  margin-top:120px;
  background: url("data:image/jpeg;base64,") no-repeat 50%/70% rgba(0, 0, 0, 0.1);
  color: #fff;
  box-shadow: inset 0 0 0 1px rgba(255, 255, 255, 0.5);
}

.bb::before, .bb::after {
  content: '';
  z-index: -1;
  margin: -5%;
  box-shadow: inset 0 0 0 2px;
  animation: clipMe 8s linear infinite;
}
.bb::before {
  animation-delay: -4s;
}
.bb:hover::after, .bb:hover::before {
  background-color: rgba(255, 0, 0, 0.3);
}

@keyframes clipMe {
  0%, 100% {
    clip: rect(0px, 170px, 2px, 0px);
  }
  25% {
    clip: rect(0px, 2px, 170px, 0px);
  }
  50% {
    clip: rect(168px, 170px, 170px, 0px);
  }
  75% {
    clip: rect(0px, 170px, 170px, 168px);
  }
}

page {
  position: relative;
  width: 100%;
  height: 100%;
  background:linear-gradient(to bottom,#0E7EE6,rgb(137,197,251));
}


